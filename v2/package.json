{
  "name": "postco-shopify",
  "version": "2.0.0",
  "description": "Integration script for shopify plugin",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack --watch-stdin --progress --color",
    "compile": "babel ./src --out-dir ./build --source-maps --presets es2015 --plugins babel-plugin-add-module-exports --transform && uglifyjs ./build/index.js --compress --mangle --output ./build/index.min.js --source-map ./build/index.min.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PostCo/postco-shopify.git"
  },
  "keywords": [
    "postco",
    "shopify"
  ],
  "author": "Hafiz Khairudin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PostCo/postco-shopify/issues"
  },
  "homepage": "https://github.com/PostCo/postco-shopify",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.13.0",
    "uglify-js": "2.6.1",
    "watch": "0.16.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.11.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "xcomponent": "^1.0.48"
  }
}
